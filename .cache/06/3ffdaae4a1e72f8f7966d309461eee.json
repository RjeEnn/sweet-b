nal":{"line":56,"column":15}}],"sources":{"../src/components/CartItem.js":"import React, {useState, useEffect} from \"react\";\nimport leftArrow from \"../res/left-arrow.svg\" \nimport rightArrow from \"../res/right-arrow.svg\" \nimport xBtn from \"../res/x-btn.svg\"\n\nconst CartItem = ({ item, cart }) => {\n  const [amount, setAmount] = useState(item.amount);\n  const [remove, setRemove] = useState(false);\n  const [productsCart, setProductsCart] = cart;\n  useEffect(()=>{\n    let cart = JSON.parse(localStorage.getItem('cart'));\n    for (let i of cart){\n      if(i.name===item.name){\n        i.amount = amount;\n      }\n    }\n    localStorage.setItem('cart', JSON.stringify(cart));\n    setProductsCart(cart)\n  }, [amount])\n\n  useEffect(() =>{\n    let cart = JSON.parse(localStorage.getItem('cart'));\n    if(remove){\n      let newCart = cart.filter(i => i.name !== item.name)\n      localStorage.setItem('cart', JSON.stringify(newCart));\n      setProductsCart(newCart)\n    }\n  }, [remove])\n  return (\n    <div className=\"cart-item\">\n      <div className=\"cart-img-con\">\n        <img src={item.image} alt=\"item-img\" className=\"cart-item-img\"/>\n      </div>\n      <div className=\"cart-desc-con\">\n        <h3>{item.name}</h3>\n        <p>{item.price}</p>\n      </div>\n      <div className=\"cart-amount-con\">\n        <input type=\"image\" src={leftArrow} alt=\"down\" onClick={()=> {\n          if(amount > 1){\n            setAmount(amount-1)\n          }\n        }}/>\n        <p>{amount}</p>\n        <input type=\"image\" src={rightArrow} alt=\"up\" onClick={() => {\n          setAmount(amount+1)\n        }}/>\n      </div>\n        <input type=\"image\" src={xBtn} alt=\"remove\" onClick={() => {\n          setRemove(true)\n        }}/>\n    </div>\n  );\n};\n\nexport default CartItem;\n"},"lineCount":null}},"error":null,"hash":"4895bdb60e2d3b13a9f77635cf1d237d","cacheData":{"env":{}}}